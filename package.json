{
  "name": "fonden-doner-ui",
  "version": "1.0.0",
  "description": "Donation UI for fonden Blockchain",
  "main": "src/server/index.js",
  "engines": {
    "node": "^8.0.0"
  },
  "scripts": {
    "bcdev": "BLOCKCHAIN__ENABLED=true npm-run-all --parallel dev:*",
    "dev": "npm-run-all --parallel dev:*",
    "dev:client": "webpack-dev-server --mode development --open",
    "dev:server": "nodemon --exec babel-node src/server/index.js",
    "build": "npm-run-all clean build:*",
    "clean": "rm -rf dist",
    "build:client": "webpack --mode production",
    "build:server": "NODE_ENV=production babel src/server --copy-files -d dist",
    "eslint": "npm-run-all eslint:*",
    "eslint:client": "eslint -c .eslintrc-client --ignore-path .eslintignore-client .",
    "eslint:server": "eslint -c .eslintrc-server --ignore-path .eslintignore-server .",
    "start": "node dist/index.js",
    "test": "echo 'no tests yet'"
  },
  "author": "Bilal Jan",
  "license": "",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "carbon-components": "^9.91.5",
    "carbon-components-react": "^6.116.4",
    "carbon-icons": "^7.0.7",
    "event-stream": "^4.0.1",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "log4js": "^3.0.6",
    "morgan": "^1.9.1",
    "nconf": "^0.10.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.1",
    "react-breadcrumbs-dynamic": "^1.1.1",
    "react-dom": "^16.6.1",
    "react-redux": "^5.1.0",
    "react-router-config": "^4.4.0-beta.6",
    "react-router-dom": "^4.3.1",
    "reactjs-popup": "^1.3.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "shortid": "^2.2.14"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.6",
    "npm-run-all": "^4.1.3",
    "pre-commit": "^1.2.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "pre-commit": [
    "eslint"
  ]
}
